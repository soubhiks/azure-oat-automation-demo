
pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World!!!s'
                script{
                    if(isUnix()){
                           print "Unixsssssssssssssssssssssssssssss"
                    }else{
                        print "Not Unixssssssssssssss"
                    }
                }
            }
        }
    }
}
